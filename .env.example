# Environment settings
NODE_ENV=development

# Application settings
LOG_LEVEL=debug
DEBUG=false

# Git settings
GIT_DEFAULT_BRANCH=main
GH_TOKEN=

# GitHub and CI/CD settings - Substitua pelos tokens reais no arquivo .env
# GitHub Personal Access Token (permissões necessárias: repo, workflow, issues, pull_requests, packages)
GH_TOKEN=

# Token para publicação no npm Registry
NPM_TOKEN=

# Token para análises SonarCloud (opcional)
SONAR_TOKEN=

# Token para relatórios de cobertura Codecov (opcional)
CODECOV_TOKEN=

# Output settings
OUTPUT_DIR=./reports
DEFAULT_FORMAT=markdown
